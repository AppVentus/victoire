<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <parameters>
        <parameter key="victoire_seo.pageSeoType.class">Victoire\Bundle\SeoBundle\Form\PageSeoType</parameter>
    </parameters>
    <services>
        <service id="form.type.seo.page" class="%victoire_seo.pageSeoType.class%">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <tag name="form.type" alias="seo_page" />
        </service>
    </services>

</container>
