{
    "name": "victoire/{{ widget|lower }}-{{ theme|lower }}-theme",
    "type": "symfony-bundle",
    "description": "Victoire Cms {{ widget }} {{ theme }} theme",
    "keywords": ["cms", "victoire", "{{ theme }}", "{{ widget }}", "theme"],
    "homepage": "http://appventus.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Your name",
            "email": "you@company.com"
        }
    ],
    "require": {
        "php"                      : ">=5.3.3",
        "symfony/framework-bundle" : "~2.3",
        "victoire/victoire"        : "dev-master"
    },
    "autoload": {
        "psr-0": { "Victoire\\Theme\\{{ widget }}\\{{ theme }}\\{{ bundle }}": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Victoire/Theme/{{ widget }}/{{ theme }}/{{ bundle }}"
}
