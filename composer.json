{
    "name": "victoire/victoire",
    "type": "symfony-bundle",
    "description": "Victoire Bundles components",
    "keywords": ["cms", "victoire", "bundle"],
    "homepage": "http://appventus.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Leny Bernard",
            "email": "leny@appventus.com"
        }
    ],
    "require": {
        "php"                                 : ">=5.3.3",
        "symfony/symfony"                     : ">=2.4.2",
        "doctrine/orm"                        : "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle"            : ">=1.2",
        "twig/extensions"                     : "~1.0",
        "symfony/assetic-bundle"              : "~2.3",
        "symfony/swiftmailer-bundle"          : "~2.3",
        "symfony/monolog-bundle"              : "~2.4",
        "sensio/distribution-bundle"          : ">2.3",
        "sensio/framework-extra-bundle"       : "~3.0",
        "sensio/generator-bundle"             : "~2.3",
        "incenteev/composer-parameter-handler": "~2.0",

        "doctrine/doctrine-fixtures-bundle"   : "dev-master",
        "doctrine/data-fixtures"              : "dev-master",
        "knplabs/knp-menu"                    : "2.0.*@dev",
        "knplabs/knp-menu-bundle"             : "2.0.*@dev",
        "jms/translation-bundle"              : "1.1.*@dev",
        "jms/di-extra-bundle"                 : "1.4.*@dev",
        "stof/doctrine-extensions-bundle"     : "1.1.*@dev",
        "liip/imagine-bundle"                 : "1.0.*@dev",
        "knplabs/knp-gaufrette-bundle"        : ">=0.1.7",
        "friendsofsymfony/jsrouting-bundle"   : "~1.1",
        "friendsofsymfony/user-bundle"        : "2.0.*@dev",
        "appventus/ajax-bundle"               : ">=1.1.0",
        "appventus/alertify-bundle"           : "2.3.*@dev",
        "appventus/shortcuts-bundle"          : "dev-master",
        "appventus/assetic-injector-bundle"   : "dev-master",

        "victoire/filter-widget"              : "dev-master"
    },
    "require-dev": {
        "behat/behat":                              "2.4.*@dev",
        "behat/mink":                               "1.5.*",
        "behat/mink-extension":                     "*",
        "behat/mink-sahi-driver":                   "*",
        "behat/mink-goutte-driver":                 "*",
        "behat/mink-browserkit-driver":             "*@stable",
        "behat/mink-selenium2-driver":              "1.1.*",
        "behat/mink-zombie-driver":                 "*",
        "behat/symfony2-extension":                 "*@dev",
        "vipsoft/doctrine-data-fixtures-extension": "dev-master",
        "phpunit/phpunit":                          "3.7.*"
    },
    "replace": {
        "victoire/blog-bundle": "self.version",
        "victoire/core-bundle": "self.version",
        "victoire/dashboard-bundle": "self.version",
        "victoire/form-bundle": "self.version",
        "victoire/media-bundle": "self.version",
        "victoire/page-bundle": "self.version",
        "victoire/seo-bundle": "self.version"
    },
    "autoload": {
        "psr-0": { "Victoire": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Victoire"
}
