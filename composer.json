{
    "name": "victoire/victoire",
    "type": "symfony-bundle",
    "description": "Victoire Bundles components",
    "keywords": ["cms", "victoire", "bundle"],
    "homepage": "http://appventus.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Leny Bernard",
            "email": "leny@appventus.com"
        }
    ],
    "require": {
        "php"                                 : ">=5.3.3",
        "symfony/symfony"                     : "v2.4.2",
        "doctrine/orm"                        : "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle"            : "~1.2",
        "twig/extensions"                     : "~1.0",
        "symfony/assetic-bundle"              : "~2.3",
        "symfony/swiftmailer-bundle"          : "~2.3",
        "symfony/monolog-bundle"              : "~2.4",
        "sensio/distribution-bundle"          : "~2.3",
        "sensio/framework-extra-bundle"       : "~3.0",
        "sensio/generator-bundle"             : "~2.3",
        "incenteev/composer-parameter-handler": "~2.0",

        "knplabs/knp-menu"                    : "2.0.*@dev",
        "knplabs/knp-menu-bundle"             : "2.0.*@dev",
        "jms/translation-bundle"              : "1.1.*@dev",
        "jms/di-extra-bundle"                 : "1.4.*@dev",
        "stof/doctrine-extensions-bundle"     : "1.1.*@dev",
        "liip/imagine-bundle"                 : "1.0.*@dev",
        "knplabs/knp-gaufrette-bundle"        : "0.2.*@dev",
        "friendsofsymfony/jsrouting-bundle"   : "~1.1",
        "friendsofsymfony/user-bundle"        : "2.0.*@dev",
        "appventus/ajax-bundle"               : "1.1.*@dev",
        "appventus/alertify-bundle"           : "dev-master",
        "appventus/shortcuts-bundle"          : "dev-master",
        "appventus/assetic-injector-bundle"   : "dev-master"
    },
    "replace": {
        "victoire/blog-bundle": "self.version",
        "victoire/core-bundle": "self.version",
        "victoire/cms-bundle": "self.version",
        "victoire/dashboard-bundle": "self.version",
        "victoire/form-bundle": "self.version",
        "victoire/media-bundle": "self.version",
        "victoire/page-bundle": "self.version",
        "victoire/seo-bundle": "self.version"
    },
    "autoload": {
        "psr-0": { "Victoire": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Victoire",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
